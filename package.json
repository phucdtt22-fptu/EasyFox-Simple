{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"npm run server:dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server:dev": "cd server && npm install && npm run dev",
    "server:start": "cd server && npm start",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --rmi all",
    "prod:setup": "cp .env.production.example .env.production",
    "prod:deploy": "./deploy.sh",
    "health:check": "curl -f http://localhost:3001/health && curl -f http://localhost:3000"
  },
  "dependencies": {
